  <body>
    <nav class="navbar bg-dark">
      <div class="container-fluid">
        <span class="navbar-brand mb-0 text-light h1">Shoti API • <%= count %></span>
        <button onclick="window.location.href='/leaderboard'" class="btn btn-primary">Top Users</button>
      </div>
    </nav>
    <div class="modal fade" id="modal">
      <div class="modal-dialog mt-5">
        <div class="modal-content">
          <div class="modal-header">
            <h1 class="modal-title fs-5" id="exampleModalLabel">Apikey Info</h1>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body" id="modal-info"></div>
          <div class="modal-footer">
            <button id="close" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
            <button id="copy" class="btn btn-primary">Copy Apikey</button>
          </div>
        </div>
      </div>
    </div>
    <div class="container mt-3">
      <div class="mb-2">
        <label for="name">Name</label>
        <input type="text" class="form-control" name="username" id="username" placeholder="Enter your name" required>
      </div>
      <div class="h-captcha" data-sitekey="b031f031-0950-485b-9744-d9a353c5a789"></div>
      <button id="gen-key" class="btn btn-primary mt-2 w-100">Generate Key</button>
